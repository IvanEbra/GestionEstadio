                    <Image Source="bin/Debug/Iconos/equipo.jpg"/>
                </MenuItem.Icon>
            </MenuItem>
            <MenuItem x:Name="menuEstadio" Header="Estadio" Click="menuEstadio_Click_3" IsEnabled="False">
                <MenuItem.Icon>
                    <Image Source="bin/Debug/Iconos/estadioicono.jpg"/>
                </MenuItem.Icon>
            </MenuItem>
            <MenuItem x:Name="menuJornada" Header="Jornada" Click="menuJornada_Click_4" IsEnabled="False">
                <MenuItem.Icon>
                    <Image Source="bin/Debug/Iconos/jornada.jpg"/>
                </MenuItem.Icon>
            </MenuItem>
            <MenuItem x:Name="menuAyuda" Header="Ayuda" Click="MenuAyuda_Click">
                <MenuItem.Icon>
                    <Image Source="bin/Debug/Iconos/ayuda.png"/>
                </MenuItem.Icon>
            </MenuItem>
        </Menu>
        <Frame x:Name="framePaginas" Content="Frame" HorizontalAlignment="Left" Height="488
               " Margin="0,66,0,0" VerticalAlignment="Top" Width="1028" NavigationUIVisibility="Hidden"/>

    </Grid>
